{
  "name": "pagina_node",
  "version": "1.0.0",
  "description": "Repo para ver la multiples opciones para tener una pagina o api con NODEJS",
  "main": "src/index.js",
  "scripts": {
    "start": "node app/app.js",
    "dev": "nodemon --watch ./app/* --exec 'node app/app.js'",
    "dev2": "nodemon --ext js,mjs,json,html,ccs,ts,ejs --watch ./app/* --exec 'node app/app.js'",
    "dev3": "./node_modules/.bin/supervisor -k -e html,ejs -i .git/,node_modules/ -- app/app.js",
    "dev4": "nodemon --watch ./app/* --exec './node_modules/.bin/supervisor -k -e html,ejs -i .git/,node_modules/ -- app/app.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nodejs",
    "vscode"
  ],
  "author": "XixoW",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "countries-list": "^2.5.0",
    "ejs": "^3.0.2",
    "express": "^4.17.1",
    "lorem-ipsum": "^2.0.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "reload": "^3.0.4"
  }
}
